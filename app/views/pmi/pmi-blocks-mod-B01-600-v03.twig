  <body style="-webkit-text-size-adjust: 100%; -ms-text-size-adjust: 100%; margin: 0; padding: 0; font-family: 'Noto Sans', Gotham, Arial, sans-serif; font-size: 14px; line-height: 18px; color: #000000; width: 100%;" class="md-r">
    <div style="display:none;font-size:1px;line-height:1px;max-height:0px;max-width:0px;opacity:0;overflow:hidden" class="preheader">{{PREHEADER}}</div>
    <table cellpadding="0" cellspacing="0" border="0" width="600" style="margin: 0; padding: 0; text-align: left; width: 100%; min-width: 600px; line-height: 100%;" role="presentation" class="background-table has-width-600" bgcolor="#FFFFFF" valign="top">
      <tbody>
        <tr>
          <th valign="top">
            <table cellpadding="0" cellspacing="0" border="0" width="600" style="width:600px;margin:0 auto" role="presentation" class="email-content" align="center">
              <tbody>
                <tr>
                  <th valign="top">
                    <div pik-mail-block="blocks-container" style="margin:0;padding:0;background-color:{{content_bgcolor}};">
                      <table cellpadding="0" cellspacing="0" border="0" width="100%" style="min-width:100%" role="presentation">
                        <tbody>
                        {% for index in blocks|keys %}
                          {% if blocks[index].type == "img" %}
                            {% set bgclr = "transparent" %}
                            {% if blocks[index].bgcolor is defined %}
                            {% set bgclr = blocks[index].bgcolor  %}
                            {% endif %} 
                            <tr pik-mail-block="img">
                              <th valign="top" style="text-align:left;margin:0;padding:0;background-color:{{bgclr}};" class="image-container" align="left">
                                {% if blocks[index].url is defined and blocks[index].url != "#" %}<a href="{{blocks[index].url}}" target="_blank">{% endif %}<img src="{{BaseURLimg}}{{blocks[index].img}}" width="600" height="auto" align="absbottom" alt="" style="outline: none; text-decoration: none; -ms-interpolation-mode: bicubic; display: block;margin:0;padding:0;border:0;">{% if blocks[index].url is defined and blocks[index].url != "#" %}</a>{% endif %}

                              </th>
                            </tr>

                          {% elseif blocks[index].type == "rawHTML" %}
                              {% set rw1 = '' %}
                              {% set rw2 = '' %}
                              {% set rw3 = '' %}
                              {% if blocks[index].th_valign is defined and blocks[index].th_valign != "" %}
                               {% set rw1 = ' valign="' ~ blocks[index].th_valign ~ '"'  %}
                              {% endif %}
                              {% if blocks[index].th_style is defined and blocks[index].th_style != "" %}
                               {% set rw2 = ' style="' ~ blocks[index].th_style ~ '"'  %}
                              {% endif %}
                              {% if blocks[index].th_align is defined and blocks[index].th_align != "" %}
                               {% set rw3 = ' align="' ~ blocks[index].th_align ~ '"'  %}
                              {% endif %}
                              {% set raw_attribs = rw1 ~ rw2 ~ rw3 %}

                            <tr pik-mail-block="rawHTML">
                              <th {{raw_attribs|raw }}>                                 
                                {{blocks[index].html|raw}}
                              </th>
                            </tr>

                          {% elseif blocks[index].type == "imgCoverDynamicTitle" %}
                            {% set width2 = 600 - blocks[index].widthRight - blocks[index].widthLeft  %} 

                            <tr pik-mail-block="imgCoverDynamicTitle">
                              <th valign="top" style="text-align:left;margin:0;padding:0;background-color:{{blocks[index].bgcolor}};" class="image-container" align="left">
                                <table valign="middle" align="center" cellpadding="0" cellspacing="0" border="0" width="100%" style="min-width:100%;background-color:{{blocks[index].bgcolor}};padding:0;margin:0 auto;" role="presentation">
                                <tbody>
                                  <tr>
                                    <th colspan="3" valign="top" style="font-size:1px;line-height:1px;background-color:{{blocks[index].bgcolor}};" class="" width="600">
                                      {% if blocks[index].url is defined and blocks[index].url != "#" %}<a href="{{blocks[index].url}}" target="_blank">{% endif %}<img src="{{BaseURLimg}}{{blocks[index].imgTop}}" width="600" height="auto" align="absbottom" alt="" style="outline: none; text-decoration: none; -ms-interpolation-mode: bicubic; display: block;margin:0;padding:0;border:0;">{% if blocks[index].url is defined and blocks[index].url != "#" %}</a>{% endif %}
                                    </th>
                                  </tr>
                                  <tr>
                                    <th valign="top" style="font-size:1px;line-height:1px;background-color:{{blocks[index].bgcolor}};" class="" width="{{blocks[index].widthLeft}}">
                                      {% if blocks[index].url is defined and blocks[index].url != "#" %}<a href="{{blocks[index].url}}" target="_blank">{% endif %}<img src="{{BaseURLimg}}{{blocks[index].imgBottomLeft}}" width="{{blocks[index].widthLeft}}" height="auto" align="absbottom" alt="" style="outline: none; text-decoration: none; -ms-interpolation-mode: bicubic; display: block;margin:0;padding:0;border:0;">{% if blocks[index].url is defined and blocks[index].url != "#" %}</a>{% endif %}                                      
                                    </th>
                                    <th style="text-align:{{blocks[index].align}};width:{{width2}}px;" width="{{width2}}" >
                                        {# <span style="margin: 0 auto;display:block;text-align:center;">
                                        {{blocks[index].p|raw}}
                                        </span> #}

                                            <div style="color:{{blocks[index].p_color}}; display:block; padding:0; text-align:{{blocks[index].p_align}}; line-height:{{blocks[index].p_height}}; font-size:{{blocks[index].p_fontsize}}; font-weight:{{blocks[index].p_fontweight}};" class="fallback-text">{{blocks[index].p|raw}}</div><br>

                                    </th>
                                    <th valign="top" style="font-size:1px;line-height:1px;background-color:{{blocks[index].bgcolor}};" class="" width="{{blocks[index].widthRight}}">
                                      {% if blocks[index].url is defined and blocks[index].url != "#" %}<a href="{{blocks[index].url}}" target="_blank">{% endif %}<img src="{{BaseURLimg}}{{blocks[index].imgBottomRight}}" width="{{blocks[index].widthRight}}" height="auto" align="absbottom" alt="" style="outline: none; text-decoration: none; -ms-interpolation-mode: bicubic; display: block;margin:0;padding:0;border:0;">{% if blocks[index].url is defined and blocks[index].url != "#" %}</a>{% endif %}
                                    </th>
                                  </tr>

                                </tbody>
                                </table>

                              </th>
                            </tr>

                          {% elseif blocks[index].type == "CustomHTML" %}

                            <tr pik-mail-block="CustomHTML" {% if blocks[index].trStyle is defined and blocks[index].trStyle != "" %}style="{{blocks[index].trStyle}}"{% endif %}>
                              <th valign="{{blocks[index].tdValign}}"  align="{{blocks[index].tdAlign}}" style="{{blocks[index].tdStyle}}">
                                {{blocks[index].htmlContent|raw}}
                              </th>
                            </tr>

                          {% elseif blocks[index].type == "imgNotFullSize" %}

                            {% if blocks[index].align == "center" %}
                              {% set algn = "margin:0 auto; " %}
                            {% elseif blocks[index].align == "right" %}
                              {% set algn = "margin:0; float:right;" %}
                            {% else %}
                              {% set algn = "margin:0;" %}
                            {% endif %}

                            <tr pik-mail-block="imgNotFullSize">
                              <th valign="top" style="text-align:left;margin:0;padding:0;display:block;background-color:{{blocks[index].bgColor}};" class="image-container" align="left">
                                {% if blocks[index].url is defined and blocks[index].url != "#" %}<a href="{{blocks[index].url}}" target="_blank">{% endif %}<img src="{{BaseURLimg}}{{blocks[index].img}}" width="{{blocks[index].width}}" height="auto" align="absbottom" alt="" style="outline: none; text-decoration: none; -ms-interpolation-mode: bicubic; display:block; {{algn}} padding:0;border:0;">{% if blocks[index].url is defined and blocks[index].url != "#" %}</a>{% endif %}

                              </th>
                            </tr>


                          {% elseif blocks[index].type == "framed_img" %}
                            {% if blocks[index].margin_tb is not defined %}
                              {% set margin_tb = blocks[index].margin %}
                              {% else %}
                              {% set margin_tb = blocks[index].margin_tb %}
                            {% endif %}
                            {% set width1 = 600 - 2*blocks[index].margin  %} 

                            <tr pik-mail-block="framed_img">
                              <th valign="top" style="text-align:left;margin:0;padding:0;" class="" align="left">
                                <table valign="middle" align="center" cellpadding="0" cellspacing="0" border="0" width="{{blocks[index].width}}" style="min-width:{{blocks[index].width}};background-color:{{blocks[index].list_bgcolor}}" role="presentation">
                                <tbody>
                                <tr>
                                  <th colspan="3" valign="top" style="font-size:1px;line-height:1px;background-color:{{blocks[index].margincolor}};" class="horizontal-gutter" height="{{margin_tb}}">&nbsp;</th>
                                </tr>
                                <tr>
                                  <th valign="top" style="text-align:left;font-size:1px;line-height:1px;background-color:{{blocks[index].margincolor}};" class="vertical-gutter" width="{{blocks[index].margin}}">&nbsp;</th>
                                  <th style="text-align:{{blocks[index].align}};" width="{{width1}}" >
                                      {% if blocks[index].url is defined and blocks[index].url != "#" %}<a href="{{blocks[index].url}}" target="_blank">{% endif %}<img src="{{BaseURLimg}}{{blocks[index].img}}" width="{{width1}}" height="auto" align="absbottom" alt="" style="outline: none; text-decoration: none; -ms-interpolation-mode: bicubic; display: block;margin:0;padding:0;border:0;">{% if blocks[index].url is defined and blocks[index].url != "#" %}</a>{% endif %}
                                  </th>
                                  <th valign="top" style="text-align:left;font-size:1px;line-height:1px;background-color:{{blocks[index].margincolor}};" class="vertical-gutter" width="{{blocks[index].margin}}">&nbsp;</th>
                                </tr>
                                <tr>
                                  <th colspan="3" valign="top" style="font-size:1px;line-height:1px;background-color:{{blocks[index].margincolor}};" class="horizontal-gutter" height="{{margin_tb}}">&nbsp;</th>
                                </tr>
                                </tbody>
                                </table>
                              </th>
                            </tr>

                          {% elseif blocks[index].type == "spacer" %}

                            <tr pik-mail-block="spacer" style="background-color:{{blocks[index].color_bkg}};">
                              <th valign="top" style="background-color:{{blocks[index].color_bkg}};text-align:left;font-size:1px;line-height:1px" class="horizontal-gutter" height="{{blocks[index].height}}">&nbsp;</th>
                            </tr>

                          {% elseif blocks[index].type == "cta1" %}
                            {% if blocks[index].color_bkg is defined %}
                              {% set bg_color = blocks[index].color_bkg %}
                            {% else %}
                              {% set bg_color = "transparent" %}
                            {% endif %}

                            <tr pik-mail-block="cta1" style="background-color:{{bg_color}};">
                              <th valign="middle" style="text-align:center" class="horizontal-grid">
                              <table valign="top" align=center cellpadding="0" cellspacing="0" border="0" width="100%" style="min-width:100%" role="presentation">
                                <tbody>
                                <tr>
                                  <th valign="top" style="text-align:center" class="image-container" align="center">
                                    <a href="{{blocks[index].url_cta}}" target="_blank"><img src="{{BaseURLimg}}{{blocks[index].button}}" width="{{blocks[index].widthbutton}}" height="auto" align="center" alt="" style="outline: none; text-decoration: none; -ms-interpolation-mode: bicubic; border: none; display: block; margin-left:auto;margin-right:auto;width:{{blocks[index].widthbutton}}px;"></a>
                                  </th>
                                </tr>
                                </tbody>
                              </table>
                              </th>
                            </tr>

                          {% elseif blocks[index].type == "cta2" %}
                            {% if blocks[index].color_bkg is defined %}
                              {% set bg_color = blocks[index].color_bkg %}
                            {% else %}
                              {% set bg_color = "transparent" %}
                            {% endif %}

                              <tr pik-mail-block="cta2" style="background-color:{{bg_color}};">
                                  <th valign="middle" style="text-align:center" class="horizontal-grid">
                                  <table valign="top" align=center cellpadding="0" cellspacing="0" border="0" width="100%" style="min-width:100%" role="presentation">
                                      <tbody>
                                      <tr>
                                          <th valign="top" style="text-align:right" class="image-container" align="right">
                                              <a href="{{blocks[index].url_cta1}}" target="_blank"><img src="{{BaseURLimg}}{{blocks[index].button1}}" width="{{blocks[index].widthbutton1}}" height="auto" align="right" alt="" style="outline: none; text-decoration: none; -ms-interpolation-mode: bicubic; border: none; display: block; margin-left:auto;margin-right:auto;width:{{blocks[index].widthbutton1}}px;"></a>
                                          </th>
                                          <th valign="top" style="text-align:left;font-size:1px;line-height:1px" class="vertical-gutter" width="9%">&nbsp;</th>
                                          <th valign="top" style="text-align:left" class="image-container" align="left">
                                              <a href="{{blocks[index].url_cta2}}" target="_blank"><img src="{{BaseURLimg}}{{blocks[index].button2}}" width="{{blocks[index].widthbutton2}}" height="auto" align="left" alt="" style="outline: none; text-decoration: none; -ms-interpolation-mode: bicubic; border: none; display: block; margin-left:auto;margin-right:auto;width:{{blocks[index].widthbutton2}}px;"></a>
                                          </th>
                                      </tr>
                                      </tbody>
                                  </table>
                                  </th>
                              </tr>

                          {% elseif blocks[index].type == "formold" %}

                            <tr pik-mail-block="formold" style="background-color:{{blocks[index].color_bkg}};">

                              <th valign="top" style="background-color:{{blocks[index].color_bkg}};text-align:left;font-size:12px;font-weight:{{blocks[index].fontweight}};line-height:16px;padding-left:20%;color:{{blocks[index].color_form}}" class="horizontal-gutter" height="34">
                              {{blocks[index].form}}
                                <label class="container">Solo uso IQOS.
                                  <input type="radio" checked="checked" name="opinion">
                                  <span class="checkmark"></span>
                                </label>
                                <label class="container">Uso IQOS y fumo cigarros. IQOS con más frecuencia.
                                  <input type="radio" name="opinion">
                                  <span class="checkmark"></span>
                                </label>
                                <label class="container">Uso IQOS tanto como fumo cigarros, ambos casi la misma frecuencia.
                                  <input type="radio" name="opinion">
                                  <span class="checkmark"></span>
                                </label>
                                <label class="container">Uso IQOS tanto como fumo cigarros pero fumo cigarros con mayor frecuencia.
                                  <input type="radio" name="opinion">
                                  <span class="checkmark"></span>
                                </label>
                                <label class="container">Solo fumo cigarros.
                                  <input type="radio" name="opinion">
                                  <span class="checkmark"></span>
                                </label>
                              </th>
                            </tr>

                          {% elseif blocks[index].type == "greeting" %}

                            <tr pik-mail-block="greeting" style="background-color:{{blocks[index].color_bkg}};">
                              <th valign="top" style="background-color:{{blocks[index].color_bkg}};text-align:center;font-size:28px;font-weight:{{blocks[index].fontweight}};line-height:34px;color:{{blocks[index].color_greeting}}" class="horizontal-gutter fallback-text" height="34">
                                <span class="fallback-text" style="font-family: 'Noto Sans', Gotham, Arial, sans-serif;">{{blocks[index].greeting|raw}}</span>
                              </th>
                            </tr>

                          {% elseif blocks[index].type == "cols_25_25_25_25" %}

                            <tr pik-mail-block="cols_25_25_25_25">
                              <th valign="middle" style="text-align:center;background-color:{{blocks[index].color_bkg}}" class="horizontal-grid">
                              <table valign="top" align=center cellpadding="0" cellspacing="0" border="0" width="100%" style="min-width:100%" role="presentation">
                                <tbody>
                                <tr>
                                  {% for idx in blocks[index].columns|keys %}
                                    {% if blocks[index].columns[idx].coltype == "img"  and  idx < 4 %}

                                      <th valign="top" style="text-align:center" class="image-container" align="center">
                                        <table align="center" cellpadding="0" cellspacing="0" border="0" width="150" style="min-width:150px;" role="presentation">
                                        <tbody>
                                        <tr>
                                          <th>
                                            {% if blocks[index].columns[idx].imgulr is defined and blocks[index].columns[idx].imgulr != "#" %}<a href="{{blocks[index].columns[idx].imgulr}}" target="_blank">{% endif %}<img src="{{BaseURLimg}}{{blocks[index].columns[idx].img}}" width="140" height="auto" align="center" alt="" style="outline: none; text-decoration: none; -ms-interpolation-mode: bicubic; border: none; display: block; margin-left:auto;margin-right:auto;width:140px;">{% if blocks[index].columns[idx].imgulr is defined and blocks[index].columns[idx].imgulr != "#" %}</a>{% endif %}
                                          </th>
                                        </tr>
                                        </tbody>
                                        </table>
                                      </th>
                                    {% endif %}
                                  {% endfor %}

                                </tr>
                                </tbody>
                              </table>
                              </th>
                            </tr>

                          {% elseif blocks[index].type == "cols_50_50" %}

                            <tr pik-mail-block="cols_50_50">
                              <th valign="middle" style="text-align:center;background-color:{{blocks[index].color_bkg}}" class="horizontal-grid">
                              <table valign="top" align=center cellpadding="0" cellspacing="0" border="0" width="100%" style="min-width:100%" role="presentation">
                                <tbody>
                                <tr>
                                  {% for idx in blocks[index].columns|keys %}
                                    {% if blocks[index].columns[idx].coltype == "txt_cta"  and  idx < 2 %}

                                      {% set valignblock = "top" %}
                                      {% if blocks[index].columns[idx].valign is defined %}
                                          {%  set valignblock = blocks[index].columns[idx].valign %}
                                      {% endif %}

                                      <th valign="{{valignblock}}" style="text-align:center" class="image-container" align="center">
                                        <table align="center" cellpadding="0" cellspacing="0" border="0" width="299" style="min-width:299px;" role="presentation">
                                        <tbody>
                                        <tr>
                                          <th valign="top" style="text-align:left;font-size:1px;line-height:1px" class="vertical-gutter" width="12%">&nbsp;</th>
