                                              <h1 style="color:{{blocks[index].columns[idx].h1_color}}; display:block; text-align:{{blocks[index].columns[idx].h1_align}}; line-height:{{blocks[index].columns[idx].h1_height}}; font-size:{{blocks[index].columns[idx].h1_fontsize}}; font-weight:{{blocks[index].columns[idx].h1_fontweight}};" class="fallback-text">{{blocks[index].columns[idx].h1|raw}}</h1>
                                            {% endif %}

                                            <div style="color:{{blocks[index].columns[idx].p_color}}; display:block; padding:0; text-align:{{blocks[index].columns[idx].p_align}}; line-height:{{blocks[index].columns[idx].p_height}}; font-size:{{blocks[index].columns[idx].p_fontsize}}; font-weight:{{blocks[index].columns[idx].p_fontweight}};" class="fallback-text">{{blocks[index].columns[idx].p|raw}}</div><br>

                                            {% if blocks[index].columns[idx].button != "" %}

                                              <table valign="top" align="center" cellpadding="0" cellspacing="0" border="0" width="100%" style="min-width:100%" role="presentation">
                                                <tbody>
                                                <tr>
                                                  <th valign="top" style="text-align:left;font-size:1px;line-height:1px" class="horizontal-gutter" height="{{blocks[index].columns[idx].button_topm}}">&nbsp;</th>
